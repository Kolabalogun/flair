{
  "expo": {
    "name": "Flair",
    "slug": "flair",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconn.png",
    "scheme": "flair",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/spla.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This is used to send you notifications when you are near specific locations."
      }
    },
    "android": {
      "permissions": ["RECEIVE_BOOT_COMPLETED"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adap-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ibrahim.flair"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iconn.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/iconn.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f140ff06-08af-4bf6-949e-779924aa31a8"
      }
    }
  }
}
